{
    "extends": "../tsconfig.json",
    "compilerOptions": {
      "outDir": "../dist",
      "module": "commonjs",
      "baseUrl": ".",
      "types": [
        "jasmine",
        "node"
      ]
    },
    "declaration": false,
    "noImplicitAny": false,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "ES6",
    "include": [
      "**/*.spec.ts",
      "**/*.d.ts"
    ]
  }