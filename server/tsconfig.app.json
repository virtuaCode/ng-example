{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "../dist",
    "module": "commonjs",
    "types": [
      "node",
      "express",
      "jasmine"
    ],
    "declaration": false,
    "noImplicitAny": false,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "ES6"
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}